{
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "develop"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "ignoreTests": true,
  "automergeType": "branch",
  "dependencyDashboard": false,
  "ignoreDeps": [
    "org.spigotmc:spigot",
    "org.spigotmc:spigot-api"
    "io.papermc.paper:dev-bundle",
  ]
}
